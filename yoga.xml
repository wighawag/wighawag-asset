<?xml version="1.0" encoding="utf-8" ?>
<project>
	<yoga-version>1</yoga-version>
	<compiler-version>2.10</compiler-version>
	
	<id>com.wighawag.asset</id>
	<version>0.1.2-SNAPSHOT</version>

	<main>com.wighawag.asset.PerformanceTest</main>

	<sources>
		<source path="src"/>
	</sources>
	
	<tests path="test" framework="munit"/>
	
	<targets hxml="compile">
		
		<target name="swf">
			<param line="-swf-version 11"/>
		</target>
		
		<target name="cpp"/>

		<target name="js"/>		

	</targets>
	
	
	<config-file output="project.nmml" template="nmml.template"/>

	<dependencies>
		<repository version="0.1.1" id="com.wighawag.report.Report"/>
        	<repository version="0.1.0" id="com.wighawag.wighawag-promhx"/>
		<repository version="0.1.1" id="com.wighawag.utils"/>
		<haxelib name="tink_macros" version="1.0.2"/>
		<haxelib name="nme"/>
	</dependencies>
</project>
